<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="evenNumber" class="io.github.woukies.p1207.EvenNumber"></bean>
	
	<bean id="myEvenNumber" class="io.github.woukies.p1207.MyEvenNumber">
		<property name="evenNumber" ref="evenNumber"></property>
		<property name="number" value="10"></property>
	</bean>
	
</beans>
