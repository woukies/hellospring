<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id="calculator" class="io.github.woukies.Calculator"></bean>
	<bean id="myCalculator" class="io.github.woukies.MyCalculator">
		<property name="calculator">
			<ref bean="calculator"/>
		</property>
		<property name="firstNumber" value="10"></property>
		<property name="secondNumber" value="2"></property>
	</bean>

</beans>
