<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="circle1" class="io.github.woukies.q5_1.Circle">
		<constructor-arg>
			<value>10</value>
		</constructor-arg>
	</bean>

	<bean id="circle2" class="io.github.woukies.q5_1.Circle">
		<constructor-arg>
			<value>5</value>
		</constructor-arg>
	</bean>
	
	<bean id="circleInfo" class="io.github.woukies.q5_1.CircleInfo">
		<constructor-arg>
			<ref bean="circle1"></ref>
		</constructor-arg>
	</bean>

</beans>
