<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="median" class="io.github.woukies.q3_8.Median"></bean>
	<bean id="myMedian" class="io.github.woukies.q3_8.MyMedian">
		<property name="median">
			<ref bean="median" />
		</property>
		<property name="a" value="20"></property>
		<property name="b" value="100"></property>
		<property name="c" value="33"></property>
	</bean>

</beans>
